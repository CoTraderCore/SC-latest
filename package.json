  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "deploy": "yarn build && netlify deploy",
    "deploy-server:rinkeby": "pm2 deploy rinkeby update",
    "truffle": "truffle",
    "truffle:develop": "truffle develop",
    "truffle:console": "truffle console",
    "truffle:compile": "truffle compile && yarn update-abi-folder:local",
    "truffle:migrate": "truffle migrate --reset && yarn update-abi-folder:local",
    "update-abi-folder:local": "node scripts/createPublicABIFiles.js --network local",
    "update-abi-folder:rinkeby": "node scripts/createPublicABIFiles.js --network rinkeby",
    "update-abi-folder:ropsten": "node scripts/createPublicABIFiles.js --network ropsten",
    "truffle:test": "truffle test --network test",
    "truffle:test:full": "concurrently 'yarn truffle:develop' 'yarn truffle:test'",
    "coverage": "scripts/coverage.sh",
    "deploy:rinkeby": "truffle migrate --network rinkeby",
    "deploy:rinkeby-infura": "truffle migrate --reset --network rinkebyInfura && yarn update-abi-folder:rinkeby",
    "deploy:ropsten-infura": "truffle migrate --reset --network ropstenInfura && yarn update-abi-folder:ropsten",
    "ganache": "ganache-cli -p 9545 -i 4447 -d -m \"candy maple cake sugar pudding cream honey rich smooth crumble sweet treat\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:sol": "solium -d .",
    "lint:sol:fix": "solium -d . --fix",
    "lint:all": "yarn lint && yarn lint:sol",
    "lint:all:fix": "yarn lint:fix && yarn lint:sol:fix",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/addon-storyshots": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.1.1",
    "babel-plugin-import": "^1.6.5",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.1",
    "debug": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lerna": "^2.10.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.0",
    "react-test-renderer": "^16.3.2",
    "solidity-coverage": "^0.4.15",
    "solium": "^1.1.5",
    "standard": "^11.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
