    "react": "^16.2.0",
    "react-apollo": "^2.1.3",
    "react-app-rewire-antd-theme": "^1.0.8",
    "react-blockies": "^1.2.2",
    "react-dom": "^16.2.0",
    "react-ga": "^2.5.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
